<?xml version="1.0" encoding="UTF-8"?>
<package packagerversion="1.9.2" version="2.0" xmlns="http://pear.php.net/dtd/package-2.0" xmlns:tasks="http://pear.php.net/dtd/tasks-1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://pear.php.net/dtd/tasks-1.0 http://pear.php.net/dtd/tasks-1.0.xsd http://pear.php.net/dtd/package-2.0 http://pear.php.net/dtd/package-2.0.xsd">
 <name>Horde_Compress_Fast</name>
 <channel>pear.horde.org</channel>
 <summary>Fast Compression Library</summary>
 <description>Provides compression suitable for packing strings on-the-fly in PHP code (as opposed to more resource-intensive compression algorithms such as DEFLATE).</description>
 <lead>
  <name>Michael Slusarz</name>
  <user>slusarz</user>
  <email>slusarz@horde.org</email>
  <active>yes</active>
 </lead>
 <date>2014-07-08</date>
 <version>
  <release>1.0.4</release>
  <api>1.0.0</api>
 </version>
 <stability>
  <release>stable</release>
  <api>stable</api>
 </stability>
 <license uri="http://www.horde.org/licenses/lgpl21">LGPL-2.1</license>
 <notes>
* 
 </notes>
 <contents>
  <dir baseinstalldir="/" name="/">
   <dir name="doc">
    <dir name="Horde">
     <dir name="Compress">
      <dir name="Fast">
       <file name="COPYING" role="doc" />
      </dir> <!-- /doc/Horde/Compress/Fast -->
     </dir> <!-- /doc/Horde/Compress -->
    </dir> <!-- /doc/Horde -->
   </dir> <!-- /doc -->
   <dir name="lib">
    <dir name="Horde">
     <dir name="Compress">
      <dir name="Fast">
       <file name="Base.php" role="php" />
       <file name="Exception.php" role="php" />
       <file name="Lz4.php" role="php" />
       <file name="Lzf.php" role="php" />
       <file name="Null.php" role="php" />
      </dir> <!-- /lib/Horde/Compress/Fast -->
      <file name="Fast.php" role="php" />
     </dir> <!-- /lib/Horde/Compress -->
    </dir> <!-- /lib/Horde -->
   </dir> <!-- /lib -->
   <dir name="test">
    <dir name="Horde">
     <dir name="Compress">
      <dir name="Fast">
       <file name="AllTests.php" role="test" />
       <file name="bootstrap.php" role="test" />
       <file name="CompressFastTest.php" role="test" />
       <file name="phpunit.xml" role="test" />
      </dir> <!-- /test/Horde/Compress/Fast -->
     </dir> <!-- /test/Horde/Compress -->
    </dir> <!-- /test/Horde -->
   </dir> <!-- /test -->
  </dir> <!-- / -->
 </contents>
 <dependencies>
  <required>
   <php>
    <min>5.3.0</min>
    <max>6.0.0alpha1</max>
    <exclude>6.0.0alpha1</exclude>
   </php>
   <pearinstaller>
    <min>1.7.0</min>
   </pearinstaller>
   <package>
    <name>Horde_Exception</name>
    <channel>pear.horde.org</channel>
    <min>2.0.0</min>
    <max>3.0.0alpha1</max>
    <exclude>3.0.0alpha1</exclude>
   </package>
  </required>
  <optional>
   <package>
    <name>Horde_Test</name>
    <channel>pear.horde.org</channel>
    <min>2.1.0</min>
    <max>3.0.0alpha1</max>
    <exclude>3.0.0alpha1</exclude>
   </package>
   <package>
    <name>horde_lz4</name>
    <channel>pear.horde.org</channel>
    <providesextension>horde_lz4</providesextension>
   </package>
   <package>
    <name>lzf</name>
    <channel>pecl.php.net</channel>
    <min>1.5.2</min>
    <providesextension>lzf</providesextension>
   </package>
  </optional>
 </dependencies>
 <phprelease>
  <filelist>
   <install as="COPYING" name="doc/Horde/Compress/Fast/COPYING" />
   <install as="Horde/Compress/Fast.php" name="lib/Horde/Compress/Fast.php" />
   <install as="Horde/Compress/Fast/Base.php" name="lib/Horde/Compress/Fast/Base.php" />
   <install as="Horde/Compress/Fast/Exception.php" name="lib/Horde/Compress/Fast/Exception.php" />
   <install as="Horde/Compress/Fast/Lz4.php" name="lib/Horde/Compress/Fast/Lz4.php" />
   <install as="Horde/Compress/Fast/Lzf.php" name="lib/Horde/Compress/Fast/Lzf.php" />
   <install as="Horde/Compress/Fast/Null.php" name="lib/Horde/Compress/Fast/Null.php" />
   <install as="Horde/Compress/Fast/AllTests.php" name="test/Horde/Compress/Fast/AllTests.php" />
   <install as="Horde/Compress/Fast/bootstrap.php" name="test/Horde/Compress/Fast/bootstrap.php" />
   <install as="Horde/Compress/Fast/CompressFastTest.php" name="test/Horde/Compress/Fast/CompressFastTest.php" />
   <install as="Horde/Compress/Fast/phpunit.xml" name="test/Horde/Compress/Fast/phpunit.xml" />
  </filelist>
 </phprelease>
